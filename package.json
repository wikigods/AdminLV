{
  "name": "admin-lte",
  "description": "Responsive open source admin dashboard and control panel.",
  "version": "3.2.0",
  "license": "MIT",
  "author": "Colorlib <https://colorlib.com>",
  "main": "dist/js/adminlte.min.js",
  "scripts": {
    "dev": "npm-run-all --parallel watch docs-serve",
    "css": "npm-run-all css-compile css-prefix css-rtl css-minify",
    "css-compile": "sass --style expanded --load-path=\"node_modules\" --source-map --embed-sources --no-error-css build/scss/:dist/css/ --silence-deprecation=color-functions,global-builtin,mixed-decls,import",
    "css-rtl": "cross-env NODE_ENV=RTL postcss --config build/config/postcss.config.mjs --dir \"dist/css\" --ext \".rtl.css\" \"dist/css/*.css\" \"!dist/css/*.min.css\" \"!dist/css/*.rtl.css\"",
    "css-lint": "stylelint \"build/scss/**/*.scss\" --cache --cache-location .cache/.stylelintcache --rd",
    "css-minify": "npm-run-all --aggregate-output --parallel css-minify-*",
    "css-minify-main": "cleancss -O1 --format breakWith=lf --with-rebase --source-map --source-map-inline-sources --output dist/css/ --batch --batch-suffix \".min\" \"dist/css/*.css\" \"!dist/css/*.min.css\" \"!dist/css/*rtl*.css\"",
    "css-minify-rtl": "cleancss -O1 --format breakWith=lf --with-rebase --source-map --source-map-inline-sources --output dist/css/ --batch --batch-suffix \".min\" \"dist/css/*rtl.css\" \"!dist/css/*.min.css\"",
    "css-prefix": "postcss --config src/config/postcss.config.mjs --replace \"dist/css/*.css\" \"!dist/css/*.rtl*.css\" \"!dist/css/*.min.css\"",
    "js": "npm-run-all js-compile js-minify",
    "js-compile": "rollup --config src/config/rollup.config.js --sourcemap",
    "js-lint": "eslint --cache --cache-location .cache/.eslintcache --report-unused-disable-directives .",
    "bundlewatch": "bundlewatch --config .bundlewatch.config.json",
    "lockfile-lint": "lockfile-lint --allowed-hosts npm --allowed-schemes https: --empty-hostname false --type npm --path package-lock.json",
    "docs-compile": "cd docs/ && bundle exec jekyll build -d ../docs_html",
    "docs-lint": "node build/npm/vnu-jar.js",
    "docs-format": "prettier --write \"dist/pages/**/*.html\"",
    "docs-serve": "npm-run-all compile docs-prepare && cd docs/ && bundle exec jekyll serve",
    "assets": "node build/config/assets.config.mjs",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel js-lint css-lint docs-lint lockfile-lint",
    "compile": "npm-run-all docs-compile docs-format assets css js",
    "production": "npm-run-all css-compile",
    "watch": "concurrently \"npm:watch-*\"",
    "watch-css": "nodemon --watch build/scss/ --ext scss --exec \"npm-run-all css-lint css-compile css-rtl\"",
    "watch-js": "nodemon --watch build/js/ --ext js --exec \"npm-run-all js-lint js-compile\"",
    "watch-assets": "nodemon --watch build/assets/ --exec \"npm run assets\""
  },
  "keywords": [
    "css",
    "sass",
    "responsive",
    "admin",
    "template",
    "theme",
    "framework",
    "control-panel",
    "dashboard"
  ],
  "homepage": "https://adminlte.io",
  "style": "dist/css/adminlte.css",
  "sass": "build/scss/adminlte.scss",
  "repository": {
    "type": "git",
    "url": "git://github.com/ColorlibHQ/AdminLTE.git"
  },
  "bugs": {
    "url": "https://github.com/ColorlibHQ/AdminLTE/issues"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.4",
    "@lgaitan/pace-progress": "^1.0.7",
    "@popperjs/core": "^2.11.8",
    "@sweetalert2/theme-bootstrap-4": "^5.0.11",
    "@ttskch/select2-bootstrap4-theme": "^1.5.2",
    "bootstrap": "~4.6.2",
    "bootstrap-colorpicker": "^3.4.0",
    "bootstrap-slider": "^11.0.2",
    "bootstrap4-duallistbox": "^4.0.2",
    "bs-custom-file-input": "^1.3.4",
    "bs-stepper": "^1.7.0",
    "chart.js": "^2.9.4",
    "codemirror": "^5.65.6",
    "datatables.net": "^1.12.1",
    "datatables.net-autofill-bs4": "^2.4.0",
    "datatables.net-bs4": "^1.12.1",
    "datatables.net-buttons-bs4": "^2.2.3",
    "datatables.net-colreorder-bs4": "^1.5.6",
    "datatables.net-fixedcolumns-bs4": "^4.1.0",
    "datatables.net-fixedheader-bs4": "^3.2.4",
    "datatables.net-keytable-bs4": "^2.7.0",
    "datatables.net-responsive-bs4": "^2.3.0",
    "datatables.net-rowgroup-bs4": "^1.2.0",
    "datatables.net-rowreorder-bs4": "^1.2.8",
    "datatables.net-scroller-bs4": "^2.0.7",
    "datatables.net-searchbuilder-bs4": "^1.3.4",
    "datatables.net-searchpanes-bs4": "^2.0.2",
    "datatables.net-select-bs4": "^1.4.0",
    "daterangepicker": "^3.1.0",
    "dropzone": "^5.9.3",
    "ekko-lightbox": "^5.3.0",
    "fastclick": "^1.0.6",
    "filterizr": "^2.2.4",
    "flag-icons": "^6.6.3",
    "flot": "^4.2.2",
    "fs-extra": "^10.1.0",
    "fullcalendar": "^5.11.0",
    "icheck-bootstrap": "^3.0.1",
    "inputmask": "^5.0.7",
    "ion-rangeslider": "^2.3.1",
    "jquery": "^3.6.0",
    "jquery-knob-chif": "^1.2.13",
    "jquery-mapael": "^2.2.0",
    "jquery-mousewheel": "^3.1.13",
    "jquery-ui-dist": "^1.13.1",
    "jquery-validation": "^1.19.5",
    "jqvmap-novulnerability": "^1.5.1",
    "jsgrid": "^1.5.3",
    "jszip": "^3.10.0",
    "moment": "^2.29.4",
    "overlayscrollbars": "^1.13.2",
    "pdfmake": "^0.2.5",
    "raphael": "^2.3.0",
    "select2": "^4.0.13",
    "sparklines": "^1.3.0",
    "summernote": "^0.8.20",
    "sweetalert2": "^11.4.20",
    "tempusdominus-bootstrap-4": "^5.39.2",
    "toastr": "^2.1.4",
    "uplot": "^1.6.22"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@rollup/plugin-babel": "^6.0.4",
    "autoprefixer": "^10.4.21",
    "browser-sync": "^3.0.3",
    "bundlewatch": "^0.4.0",
    "clean-css-cli": "^5.6.3",
    "concurrently": "^9.1.2",
    "cross-env": "^10.1.0",
    "eslint": "^9.23.0",
    "eslint-config-xo": "^0.46.0",
    "eslint-plugin-compat": "^6.0.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-unicorn": "^58.0.0",
    "lockfile-lint": "^4.14.0",
    "node-sass-package-importer": "^5.3.3",
    "nodemon": "^3.1.9",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.3",
    "postcss-cli": "^11.0.1",
    "rollup": "^4.38.0",
    "sass": "^1.86.0",
    "stylelint": "^16.17.0",
    "stylelint-config-twbs-bootstrap": "^16.0.0",
    "terser": "^5.39.0",
    "vnu-jar": "^24.10.17"
  }
}
